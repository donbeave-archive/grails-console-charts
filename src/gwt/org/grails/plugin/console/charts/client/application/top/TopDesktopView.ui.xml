<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
             xmlns:html="urn:import:org.gwtbootstrap3.client.ui.html">
    <html:Div b:id="header">
        <b:Navbar>
            <html:Span styleName="navbar-brand">Grails Charts Console</html:Span>
            <html:Div styleName="navbar-form" addStyleNames="pull-right">
                <b:Tooltip text="Could not connect to server" placement="LEFT" ui:field="connectTooltip">
                    <b:Button fontAwesomeIcon="CIRCLE" size="SMALL" ui:field="connectButton"/>
                </b:Tooltip>
            </html:Div>
        </b:Navbar>
    </html:Div>
</ui:UiBinder>