<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:b="urn:import:com.dianaui.universal.core.client.ui"
             xmlns:b.html="urn:import:com.dianaui.universal.core.client.ui.html">
    <b.html:Div b:id="header">
        <b:Navbar>
            <b.html:Span styleName="navbar-brand">Grails Charts Console</b.html:Span>
            <b.html:Div styleName="navbar-form" addStyleNames="pull-right">
                <b:Tooltip text="Could not connect to server" placement="LEFT" ui:field="connectTooltip">
                    <b:Button fontAwesomeIcon="CIRCLE" size="SMALL" ui:field="connectButton"/>
                </b:Tooltip>
            </b.html:Div>
        </b:Navbar>
    </b.html:Div>
</ui:UiBinder>